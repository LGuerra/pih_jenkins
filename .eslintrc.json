{
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "__PRODUCTION__": true,
    "__DEV__": true,
    "$": true,
    "IMAGES": true,
    "google": true,
    "canvg": true,
    "apigClient": true,
    "apigClientFactory": true,
    "apiGateway": true,
    "uritemplate": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}

