{
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "no-debugger": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "$": true,
    "google": true,
    "canvg": true,
    "debugger": true,
    "uritemplate": true,
    "describe": true,
    "it": true,
    "before": true,
    "beforeEach": true
    "ga": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}

